extend type Query {
    me: User
}

type User {
    """
    The ID of the user
    """
    id: ID!

    """
    The user's first name
    """
    firstName: String!

    """
    The user's last name
    """
    lastName: String!

    """
    The user's email address
    """
    email: String

    """
    Indicates whether the user's account has been disabled
    """
    isDisabled: Boolean

    """
    Indicates whether the user is an admin
    """
    isAdmin: Boolean
}